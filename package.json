    "angular-responsive-carousel": "^2.0.2",
    "animate.css": "^4.1.1",
    "bootstrap": "^4.3.1",
    "buffer": "^5.6.0",
    "compression": "^1.7.4",
    "core-js": "^2.6.9",
    "domino": "^2.1.4",
    "dotenv": "^6.2.0",
    "express": "^4.17.1",
    "file-saver": "^2.0.2",
    "font-awesome": "^4.7.0",
