{
  "name": "aprintis-frontend",
  "version": "2.0.0",
  "engines": {
    "node": "12"
  },
  "scripts": {
    "config": "ts-node ./scripts/set-env.ts",
    "ng": "ng",
    "start": "npm run serve:ssr",
    "serve": "ng serve",
    "serve-gl": "ng serve --host 0.0.0.0",
    "start:dev": "ng serve --aot",
    "start:no-ssr": "node server.js",
    "build": "npx patch-package && npm run build:ssr",
    "build:app": "npm run config && ng build --configuration production --aot --vendor-chunk --common-chunk --delete-output-path --buildOptimizer --subresource-integrity=false",
    "build-stats": "npm run config && ng build --configuration production --aot --vendor-chunk --common-chunk --delete-output-path --buildOptimizer --subresource-integrity=true --stats-json",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "stage-build": "git push heroku master",
    "bundle-report": "webpack-bundle-analyzer dist/aprintis-frontend/stats.json",
    "compile:server": "webpack --config webpack.server.config.js --progress --colors",
    "compile:polyfill-ie": "webpack --config webpack.polyfill.config.js --progress --colors",
    "serve:ssr": "node dist/server",
    "build:ssr": "npm run build:client-and-server-bundles && npm run compile:server",
    "build:client-and-server-bundles": "npm run build:app && ng run aprintis-frontend:server:production --bundleDependencies all"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.2.10",
    "@angular/cdk": "^15.2.9",
    "@angular/cdk-experimental": "^13.3.9",
    "@angular/common": "^15.2.10",
    "@angular/compiler": "^15.2.10",
    "@angular/core": "^15.2.10",
    "@angular/flex-layout": "^12.0.0-beta.27",
    "@angular/forms": "^15.2.10",
    "@angular/material": "^15.2.9",
    "@angular/platform-browser": "^15.2.10",
    "@angular/platform-browser-dynamic": "^15.2.10",
    "@angular/platform-server": "^15.2.10",
    "@angular/pwa": "^0.900.7",
    "@angular/router": "^15.2.10",
    "@angular/service-worker": "^15.2.10",
    "@ckeditor/ckeditor5-angular": "^4.0.0",
    "@ckeditor/ckeditor5-build-classic": "^34.2.0",
    "@ckeditor/ckeditor5-build-decoupled-document": "^34.2.0",
    "@ckeditor/ckeditor5-editor-classic": "^34.2.0",
    "@ckeditor/ckeditor5-image": "^34.2.0",
